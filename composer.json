{
    "name": "ryanvelbon/shopee-web-api-php",
    "description": "A PHP wrapper for Shopee's Web API.",
    "type": "library",
    "license": "MIT",
    "keywords": ["shopee", "ecommerce", "php client"],
    "homepage": "https://github.com/ryanvelbon/shopee-web-api-php",
    "authors": [
        {
            "name": "Ryan Vel Bon",
            "email": "ryanvelbon@gmail.com",
            "homepage": "https://ryanvelbon.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "ShopeeWebAPI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.2"
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests"
    }
}
